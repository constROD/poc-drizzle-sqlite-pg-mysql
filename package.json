{
  "name": "poc-drizzle-sqlite-pg-mysql",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "npx tsx watch ./src/index.ts --no-cache",
    "db:init": "sh ./setup/bash/db-init.sh",
    "db:start": "pnpm db:init && docker compose -f ./docker-compose.yaml up -d",
    "db:stop": "docker compose -f ./docker-compose.yaml down",
    "sqlite:generate": "npx drizzle-kit generate:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:push": "npx drizzle-kit push:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:introspect": "npx drizzle-kit introspect:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:drop": "npx drizzle-kit drop:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:check": "npx drizzle-kit check:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:up": "npx drizzle-kit up:sqlite --config=./drizzle.sqlite.config.ts",
    "sqlite:studio": "npx drizzle-kit studio --config=./drizzle.sqlite.config.ts",
    "pg:generate": "npx drizzle-kit generate:pg --config=./drizzle.pg.config.ts",
    "pg:push": "npx drizzle-kit push:pg --config=./drizzle.pg.config.ts",
    "pg:introspect": "npx drizzle-kit introspect:pg --config=./drizzle.pg.config.ts",
    "pg:drop": "npx drizzle-kit drop:pg --config=./drizzle.pg.config.ts",
    "pg:check": "npx drizzle-kit check:pg --config=./drizzle.pg.config.ts",
    "pg:up": "npx drizzle-kit up:pg --config=./drizzle.pg.config.ts",
    "pg:studio": "npx drizzle-kit studio --config=./drizzle.pg.config.ts",
    "mysql:generate": "npx drizzle-kit generate:mysql --config=./drizzle.mysql.config.ts",
    "mysql:push": "npx drizzle-kit push:mysql --config=./drizzle.mysql.config.ts",
    "mysql:introspect": "npx drizzle-kit introspect:mysql --config=./drizzle.mysql.config.ts",
    "mysql:drop": "npx drizzle-kit drop:mysql --config=./drizzle.mysql.config.ts",
    "mysql:check": "npx drizzle-kit check:mysql --config=./drizzle.mysql.config.ts",
    "mysql:up": "npx drizzle-kit up:mysql --config=./drizzle.mysql.config.ts",
    "mysql:studio": "npx drizzle-kit studio --config=./drizzle.mysql.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "mysql2": "^3.6.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/better-sqlite3": "^7.6.6",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
